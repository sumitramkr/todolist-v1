<%- include("header") -%>
  <div class="box" id="heading">
    <h1>
      <%= titleHTML %>
    </h1>
  </div>

  <div class="box">
    <% for(var i=0; i < itemHTML.length; ++i) {%>
      <div class="item">
        <input type="checkbox">
        <p>
          <%=itemHTML[i]%>
        </p>
      </div>
      <% } %>
        <form action="/" method="post" class="item">
          <input type="text" name="item_" placeholder="Add item" autocomplete="off" />
          <button type="submit" name="listName" value="<%=titleHTML%>">+</button>
        </form>
  </div>
  <%- include("footer") -%>